<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.user.mapper.UserMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.user.entity.User">        <id column="ID" property="id" />           <result column="NAME" property="name" />           <result column="BIRTHDAY" property="birthday" />       </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        ID, NAME, BIRTHDAY    </sql>    <select id="getUserList" resultMap="BaseResultMap">           select * from t_user    </select></mapper>