<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.test.mapper.TestMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.test.entity.Test">        <id column="USER_ID" property="userId" />        <result column="USER_NAME" property="userName" />        <result column="PASSWORD" property="password" />        <result column="BIRTHDAY" property="birthday" />        <result column="ACCOUNT_STATUS" property="accountStatus" />        <result column="CREATE_TIME" property="createTime" />        <result column="DEPT_ID" property="deptId" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        USER_ID, USER_NAME, PASSWORD, BIRTHDAY, ACCOUNT_STATUS, CREATE_TIME, DEPT_ID    </sql>    <select id="customPageList" resultType="com.sweet.modular.test.entity.Test"            parameterType="com.sweet.modular.test.entity.Test">        select        <include refid="Base_Column_List"/>        from t_test where 1 = 1       <if test="param.userName != null and param.userName != ''">        and USER_NAME like concat('%',#{param.userName},'%')       </if>    </select></mapper>