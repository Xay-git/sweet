<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.account.mapper.AccountMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.account.entity.Account">        <id column="ID" property="id" />        <result column="SITE_NAME" property="siteName" />        <result column="URL" property="url" />        <result column="ACCOUNT" property="account" />        <result column="PASSWORD" property="password" />        <result column="CREATE_TIME" property="createTime" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        ID, SITE_NAME, URL, ACCOUNT, PASSWORD, CREATE_TIME    </sql>    <select id="customPageList" resultType="com.sweet.modular.account.entity.Account"            parameterType="com.sweet.modular.account.entity.Account">        select        <include refid="Base_Column_List"/>        from business_account where 1 = 1       <if test="param.siteName != null and param.siteName != ''">        and SITE_NAME like concat('%',#{param.siteName},'%')       </if>        order by create_time desc    </select></mapper>