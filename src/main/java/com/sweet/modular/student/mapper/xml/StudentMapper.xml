<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.student.mapper.StudentMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.student.entity.Student">        <id column="STUDENT_ID" property="studentId" />        <result column="USER_ID" property="userId" />        <result column="DEPT_ID" property="deptId" />        <result column="STUDENT_NAME" property="studentName" />        <result column="BIRTHDAY" property="birthday" />        <result column="CREATE_TIME" property="createTime" />        <result column="PHOTOS" property="photos" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        STUDENT_ID, USER_ID, DEPT_ID, STUDENT_NAME, BIRTHDAY, CREATE_TIME, PHOTOS    </sql>    <select id="customPageList" resultType="com.sweet.modular.student.entity.Student"            parameterType="com.sweet.modular.student.entity.Student">        select        <include refid="Base_Column_List"/>        from t_student where 1 = 1       <if test="param.studentName != null and param.studentName != ''">        and STUDENT_NAME like concat('%',#{param.studentName},'%')       </if>    </select></mapper>