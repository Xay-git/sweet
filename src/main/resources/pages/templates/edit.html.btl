${cfg.begin}<% var tableId = cfg.fieldList[0].columnName; %><div class="layui-fluid" id="app">    <div class="layui-card" style="max-width: 60%;margin:30px auto;">        <div class="layui-card-body" >            <fieldset class="layui-elem-field layui-field-title" style="margin-top: 35px;">                <legend>${table.comment}</legend>            </fieldset>            <form class="layui-form" id="${package.ModuleName}Form" action="" style="margin: 35px 0 30px 20%;">              <% for(item in cfg.fieldList){ %>                <% if(item.columnName==tableId){ %>                    <input type="hidden" name="${item.columnName}">                <% }else{ %>                    <div class="layui-form-item">                        <label class="layui-form-label">${item.columnComment}<span style="color: red;">*</span></label>                        <div class="layui-input-inline">                            <input name="${item.columnName}" type="text"  placeholder="请输入${item.columnComment}" class="layui-input" lay-verify="required|" />                        </div>                    </div>                <% } %>               <% } %>                <div class="layui-form-item" style="margin:28px 30px;">                    <div class="layui-input-block">                        <button class="layui-btn" lay-filter="${package.ModuleName}Form" lay-submit>提交</button>                        <button class="layui-btn layui-btn-primary" type="reset" >重置</button>                    </div>                </div>            </form>        </div>    </div></div><script>    var vm = new Vue({        el: '#app',        data: {            msg: 'Hello World!',            info:'',        },        methods: {        },        mounted:async function() {            vm = this;            var ${tableId} = app.getUrlParam('${tableId}');            if(!app.isEmpty(${tableId})){                vm.info = await request.fetch("get${package.entityName}List",{${tableId}:${tableId}});                form.val('${package.ModuleName}Form', vm.info);            }            form.on('submit(${package.ModuleName}Form)', function (data) {                request.post("edit${table.entityName}",data.field).then(res=>{                    location.href = '${package.ModuleName}_list'                })                return false;            });            form.render();        },        updated: function () {            form.render();        }    })</script>${cfg.end}<style>    .layui-input-inline{        min-width:350px !important;    }</style>